{
  "name": "sin",
  "version": "1.0.0",
  "description": "sin",
  "type": "module",
  "main": "src/index.js",
  "scripts": {
    "dev": "sin dev play.js",
    "lint": "eslint . --ignore-pattern sin.js --ignore-pattern sin.min.js --ignore-pattern play.js",
    "bundle": "bun bundle:min && bun bundle:bundle && bun bundle:gzip",
    "bundle:min": "esbuild src/index.js --minify --format=esm --bundle --outfile=sin.min.js",
    "bundle:bundle": "esbuild src/index.js --format=esm --bundle --outfile=sin.js",
    "bundle:gzip": "gzip -c sin.min.js > sin.min.js.gz && ls -l sin.min.js.gz && rm sin.min.js.gz",
    "bundle:dev": "esbuild bin/develop/tools/index.js --minify --format=esm --bundle --outfile=bin/develop/tools/dist.js"
  },
  "bin": {
    "sin": "./bin/index.js"
  },
  "files": [
    "/bin",
    "/bundle",
    "/src",
    "/ssr",
    "/sin.js",
    "/sin.min.js",
    "/sin.svg"
  ],
  "author": "Rasmus Porsager <rasmus@porsager.com> (https://www.porsager.com)",
  "funding": {
    "type": "individual",
    "url": "https://github.com/sponsors/porsager"
  },
  "repository": "porsager/sin",
  "homepage": "https://github.com/porsager/sin",
  "bugs": "https://github.com/porsager/sin/issues",
  "keywords": [
    "view",
    "framework",
    "sin.js",
    "sin",
    "simple",
    "css",
    "html",
    "javascript",
    "not-react",
    "not-svelte",
    "not-vue"
  ],
  "dependencies": {
    "esbuild": "0.19.5",
    "ey": "github:porsager/ey#4c38b7e8c76ed04afec06043fcd95128d6e784a6",
    "prexit": "2.0.2"
  }
}
